{
  "initial:before": {
    "loopback#favicon": {}
  },
  "initial": {
    "../middleware/healthyFilter.js": {
      "paths": [
         "/itm/healthy"
       ],
      "enabled": true
    },
    "../middleware/versionFilter.js": {
      "paths": [
         "/itm/version"
       ],
      "enabled": true
    },
    "compression": {},
    "cors": {
      "params": {
        "origin": true,
        "credentials": true,
        "maxAge": 86400
      }
    },
    "helmet#xssFilter": {},
    "helmet#hsts": {
      "params": {
        "maxAge": 0,
        "includeSubdomains": true
      }
    },
    "helmet#hidePoweredBy": {},
    "helmet#ieNoOpen": {},
    "helmet#noSniff": {},
    "helmet#noCache": {
      "enabled": false
    }
  },
  "session": {},
  "auth": {},
  "parse": {
    "body-parser#json": {},
    "body-parser#urlencoded": {"params": { "extended": false }},
    "cookie-parser": {},
    "../middleware/g11nFilter": {
      "paths": [
        "${restApiRoot}"
      ]
    }
  },
  "routes": {
    "../middleware/auth/filter/userFilter.js": {
      "paths": [
        "${restApiRoot}"
      ]
    },
    "../middleware/mock/middlewares#getLoginUser": {
      "enabled": false
    },
    "../middleware/mock/middlewares#getProfile": {
      "enabled": false
    },
    "../middleware/mock/middlewares#getPeopleIdsMapping": {
      "enabled": false
    },
    "../middleware/mock/middlewares#getProfilesBulk": {
      "enabled": false
    },
    "loopback#rest": {
      "paths": [
        "${restApiRoot}"
      ]
    }
  },
  "files": {
    "loopback#static": {
      "params": "$!../client"
    }
  },
  "final": {
    "loopback#urlNotFound": {}
  },
  "final:after": {
    "strong-error-handler": {}
  }
}
