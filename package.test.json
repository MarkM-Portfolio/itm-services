{
  "name": "itm-services-tester",
  "version": "0.0.1",
  "description": "ITM Services Tester",
  "main": "server.js",
  "scripts": {
    "check:lint:fix": "eslint --fix --ext .js tests/",
    "start": "cross-env ITM_SERVICES_BASE_URL=http://localhost:3000/itm TEST_SUITE=governor node ./lib-tests/jasmine-runner.js",
    "start:dev": "cross-env ITM_SERVICES_BASE_URL=http://localhost:3000/itm TEST_SUITE=governor nodemon tests/jasmine-runner.js --exec babel-node",
    "start:dev:d": "cross-env ITM_SERVICES_BASE_URL=http://localhost:3000/itm TEST_SUITE=governor nodemon tests/jasmine-runner.js --exec babel-node debug",
    "build": "rimraf ./lib-tests/ && cross-env BABEL_ENV=commonjs babel ./tests --out-dir ./lib-tests --copy-files"
  },
  "repository": {
    "type": "git",
    "url": "https://github.ibm.com/connections/itm-services.git"
  },
  "keywords": [
    "itm-services",
    "Governor",
    "Tester"
  ],
  "author": "Xiaohu He",
  "license": "ISC",
  "bugs": {
    "url": "https://github.ibm.com/connections/itm-services/issues"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.4.5",
    "babel-plugin-module-resolver": "^2.3.0",
    "babel-plugin-rewire": "1.0.0-rc-3",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "babel-template": "6.16.0",
    "babel-types": "6.19.0",
    "cross-env": "5.0.5",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-config-ess": "3.0.2",
    "eslint-import-resolver-babel-module": "^2.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "nodemon": "1.8.1",
    "rimraf": "^2.5.4"
  },
  "dependencies": {
    "jasmine": "^2.4.1",
    "jasmine-core": "2.4.1",
    "jasmine-reporters": "^2.2.0",
    "request": "^2.81.0",
    "supertest": "^1.2.0",
    "superagent-use": "^0.1.0"
  }
}
